/* components/tianfu.wxss */

.container{
    width: 750rpx;
    display: flex;
    flex-direction: column;
   align-items: center;
    /* padding:30rpx; */
    /* padding-right: 30rpx; */
}
.container>image:first-child{
    width: 690rpx;
    height: 415rpx;
}
.container>view:nth-child(2){
    padding-top: 50rpx;
    padding: 30rpx;
}
.clouds{
    display: flex;
    align-items: center;
  
}
.clouds>image{
    width: 60rpx;
    height: 30rpx;
}
.money{
    display: flex;
    flex-wrap: wrap;
    padding-top: 20rpx;
    justify-content: space-between;
}

.money>image{
    width: 320rpx;
    height: 300rpx;
}