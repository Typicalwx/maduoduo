/* components/daohang/daohang.wxss */
.swiper-block{
  height: 150rpx;
  width: 100%;
}
.swiper-item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  overflow:unset;
  width: 100rpx;
  text-align: center;
}
.slide-image{
  height:70rpx;
  width: 70rpx;
  border-radius: 50%;
  box-shadow: 0px 0px 30rpx rgba(0, 0,0,.2); 
  z-index: 1;  
  margin-left: 40rpx;
}
.active{
  transform: scale(1.5);
  transition:all .2s ease-in 0s;
  z-index: 20; 
}
.textmargin{
  width: 100%;
  margin-top: 20rpx;
  text-align: center;
  font-size: 10pt;
  color: gray;
}
